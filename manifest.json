{
	"manifest_version": 2,
	"name": "__MSG_name__",
	"description": "__MSG_desc__",
	"version": "1.0.2",
	"homepage_url": "https://github.com/pepelsbey/vk-back-off",
	"default_locale": "en",
	"icons": {
		"16": "images/icon-16.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	},
	"permissions": [
		"https://vk.com/*",
		"http://vk.com/*"
	],
	"content_scripts": [{
		"run_at": "document_end",
		"matches": [
			"https://vk.com/*",
			"http://vk.com/*",
			"https://new.vk.com/*",
			"http://new.vk.com/*"
		],
		"css": [
			"styles.css"
		]
	}]
}
